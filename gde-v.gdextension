[configuration]

entry_symbol = "gdev_library_init"
compatibility_minimum = "4.3"

[libraries]

macos.debug = "./bin/libgdgde-v.macos.template_debug.framework"
macos.release = "./bin/libgdgde-v.macos.template_release.framework"

windows.debug.x86_32 = "./bin/libgde-v.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "./bin/libgdgde-v.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "./bin/libgde-v.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "./bin/libgdgde-v.windows.template_release.x86_64.dll"
windows.debug.arm64 = "./bin/libgde-v.windows.template_debug.arm.dll"
windows.release.arm64 = "./bin/libgdgde-v.windows.template_release.arm64.dll"

linux.debug.x86_32 = "./bin/libgdgde-v.linux.template_debug.x86_32.so"
linux.release.x86_32 = "./bin/libgdgde-v.linux.template_release.x86_32.so"
linux.debug.x86_64 = "./bin/libgdgde-v.linux.template_debug.x86_64.so"
linux.release.x86_64 = "./bin/libgdgde-v.linux.template_release.x86_64.so"
linux.debug.arm32 = "./bin/libgdgde-v.linux.template_debug.arm32.so"
linux.release.arm32 = "./bin/libgdgde-v.linux.template_release.arm32.so"
linux.debug.arm64 = "./bin/libgdgde-v.linux.template_debug.arm64.so"
linux.release.arm64 = "./bin/libgdgde-v.linux.template_release.arm64.so"
linux.debug.rv64 = "./bin/libgdgde-v.linux.template_debug.rv64.so"
linux.release.rv64 = "./bin/libgdgde-v.linux.template_release.rv64.so"

android.debug.x86_64 = "./bin/libgdgde-v.android.template_debug.x86_64.so"
android.release.x86_64 = "./bin/libgdgde-v.android.template_release.x86_64.so"
android.debug.arm64 = "./bin/libgdgde-v.android.template_debug.arm64.so"
android.release.arm64 = "./bin/libgdgde-v.android.template_release.arm64.so"

ios.debug = "./bin/libgdgde-v.ios.template_debug.xcframework"
ios.release = "./bin/libgdgde-v.ios.template_release.xcframework"

web.debug.threads.wasm32 = "./bin/libgdgde-v.web.template_debug.wasm32.wasm"
web.release.threads.wasm32 = "./bin/libgdgde-v.web.template_release.wasm32.wasm"
web.debug.wasm32 = "./bin/libgdgde-v.web.template_debug.wasm32.nothreads.wasm"
web.release.wasm32 = "./bin/libgdgde-v.web.template_release.wasm32.nothreads.wasm"

[dependencies]
ios.debug = {
  "res://bin/libgodot-cpp.ios.template_debug.xcframework": ""
}
ios.release = {
  "res://bin/libgodot-cpp.ios.template_release.xcframework": ""
}
